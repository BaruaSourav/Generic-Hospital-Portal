@model IEnumerable<HMSv2.Models.DB.Physician>

@{
    ViewBag.Title = "Doctor's List";
}

<h2 class="text-center">All the Doctor's till now</h2>

<p class="text-center">
    @Html.ActionLink("Add a Doctor", "AddDoctor",null,new { @class="btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
           Full Name
         </th>
       
        <th>
            @Html.DisplayNameFor(model => model.EduQualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Experience)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EduQualification)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Experience)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PhysicianID }) |
            @Html.ActionLink("Details of the Doctor", "Details", new { id=item.PhysicianID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PhysicianID })
        </td>
    </tr>
}

</table>
<p class="text-center">
    @Html.ActionLink("See All Users", "UserList", null, new { @class = "btn btn-success" })
</p>
